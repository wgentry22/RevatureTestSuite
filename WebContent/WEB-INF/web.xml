<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>RevatureTestSuite</display-name>
	<servlet>
		<servlet-name>TesterServlet</servlet-name>
		<display-name>TesterServlet</display-name>
		<description></description>
		<servlet-class>com.revature.servlets.TesterServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>GroupServlet</servlet-name>
		<display-name>GroupServlet</display-name>
		<description></description>
		<servlet-class>com.revature.servlets.GroupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TesterServlet</servlet-name>
		<url-pattern>/GetAllTests</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GroupServlet</servlet-name>
		<url-pattern>/RunGroup/*</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>CorsFilter</filter-name>
		<filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>CorsFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
</web-app>